{
  "name": "artefatvs",
  "displayName": "artefatvs",
  "description": "Organize ang show up the most recent environment artefacts",
  "publisher": "melkzsiqueira",
  "version": "0.0.1",
  "icon": "images/resources/logo.png",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "artefatvs-explorer",
          "title": "Artefatvs",
          "icon": "images/resources/logo.svg"
        }
      ]
    },
    "views": {
      "artefatvs-explorer": [
        {
          "id": "protheus",
          "name": "Protheus",
          "visibility": "visible",
          "type": "webview"
        }
      ]
    },
    "configuration": {
      "title": "Artefatvs",
      "properties": {
        "artefatvs.token": {
          "type": "string",
          "default": "",
          "description": "Token for Artefatvs"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "jsonwebtoken": "^9.0.2"
  }
}
